      "name": "Healthcare Dashboard Design System",
      "version": "1.0",
      "description": "A comprehensive design system for medical/healthcare dashboards with modern UI patterns",
      "designStyle": "Modern, clean, professional medical interface with card-based layouts"
    },
    
    "colorPalette": {
      "primary": {
        "main": "#3B82F6",
        "light": "#60A5FA",
        "dark": "#2563EB",
        "contrast": "#FFFFFF"
      },
      "secondary": {
        "main": "#1E293B",
        "light": "#475569",
        "dark": "#0F172A"
      },
      "background": {
        "main": "#F8FAFC",
        "paper": "#FFFFFF",
        "elevated": "#FFFFFF",
        "sidebar": "#1E293B"
