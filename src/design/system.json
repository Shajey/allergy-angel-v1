    "spacing": {
      "unit": "0.25rem",
      "scale": {
        "0": "0",
        "1": "0.25rem",
        "2": "0.5rem",
        "3": "0.75rem",
        "4": "1rem",
        "5": "1.25rem",
        "6": "1.5rem",
        "8": "2rem",
        "10": "2.5rem",
        "12": "3rem",
        "16": "4rem",
        "20": "5rem",
        "24": "6rem"
      },
      "componentSpacing": {
        "cardPadding": "1.5rem",
        "sectionGap": "2rem",
        "elementGap": "1rem",
        "tightGap": "0.5rem"
      }
    },

    "borderRadius": {
      "none": "0",
      "sm": "0.25rem",
      "base": "0.5rem",
      "md": "0.75rem",
      "lg": "1rem",
      "xl": "1.25rem",
      "2xl": "1.5rem",
      "full": "9999px"
    },

    "shadows": {
      "none": "none",
      "sm": "0 1px 2px 0 rgba(0, 0, 0, 0.05)",
      "base": "0 1px 3px 0 rgba(0, 0, 0, 0.1), 0 1px 2px -1px rgba(0, 0, 0, 0.1)",
      "md": "0 4px 6px -1px rgba(0, 0, 0, 0.1), 0 2px 4px -2px rgba(0, 0, 0, 0.1)",
      "lg": "0 10px 15px -3px rgba(0, 0, 0, 0.1), 0 4px 6px -4px rgba(0, 0, 0, 0.1)",
      "xl": "0 20px 25px -5px rgba(0, 0, 0, 0.1), 0 8px 10px -6px rgba(0, 0, 0, 0.1)",
      "card": "0 1px 3px 0 rgba(0, 0, 0, 0.1), 0 1px 2px -1px rgba(0, 0, 0, 0.1)",
      "elevated": "0 4px 6px -1px rgba(0, 0, 0, 0.1), 0 2px 4px -2px rgba(0, 0, 0, 0.1)"
    },

    "zIndex": {
      "base": 0,
      "dropdown": 1000,
      "sticky": 1100,
      "fixed": 1200,
      "modalBackdrop": 9998,
      "modal": 9999,
      "popover": 10000,
      "tooltip": 10001,
      "toast": 10002
    },

    "controlHeights": {
      "sm": "2rem",
      "md": "2.5rem",
      "lg": "3rem"
    },

    "iconSizes": {
      "xs": "12px",
      "sm": "16px",
      "md": "20px",
      "lg": "24px",
      "xl": "32px"
    },

    "divider": {
      "color": "#E2E8F0",
      "thickness": "1px",
      "spacing": {
        "vertical": "1rem",
        "horizontal": "1rem"
      }
    },

    "borderUsageRules": {
      "card": {
        "border": "1px solid #E2E8F0",
        "description": "Light border for card containers"
      },
      "input": {
        "default": "1px solid #E2E8F0",
        "focus": "1px solid #3B82F6",
        "error": "1px solid #EF4444",
        "description": "Medium border for form inputs"
      },
      "table": {
        "outer": "1px solid #E2E8F0",
        "inner": "1px solid #E2E8F0",
        "row": "1px solid #E2E8F0",
        "description": "Light border for table structure"
      },
      "divider": {
        "border": "1px solid #E2E8F0",
        "description": "Used for section separators"
      },
      "dropdown": {
        "border": "1px solid #E2E8F0",
        "description": "Light border for dropdown menus"
      }
    },

    "skeleton": {
      "base": {
        "backgroundColor": "#F1F5F9",
        "backgroundGradient": "linear-gradient(90deg, #F1F5F9 0%, #E2E8F0 50%, #F1F5F9 100%)",
        "backgroundSize": "200% 100%",
        "animation": "shimmer 1.5s ease-in-out infinite",
        "borderRadius": "0.25rem"
      },
      "variants": {
        "text": {
          "height": "1rem",
          "borderRadius": "0.25rem"
        },
        "title": {
          "height": "1.5rem",
          "borderRadius": "0.25rem"
        },
        "avatar": {
          "width": "2.5rem",
          "height": "2.5rem",
          "borderRadius": "9999px"
        },
        "rectangle": {
          "width": "100%",
          "height": "8rem",
          "borderRadius": "0.5rem"
        }
      }
    },

    "emptyState": {
      "container": {
        "padding": "3rem 1.5rem",
        "textAlign": "center",
        "display": "flex",
        "flexDirection": "column",
        "alignItems": "center",
        "gap": "1rem"
