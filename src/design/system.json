        "placeholder": {
          "fontSize": "0.875rem",
          "fontWeight": 400,
          "lineHeight": 1.5,
          "color": "#94A3B8"
        },
        "helperText": {
          "fontSize": "0.75rem",
          "fontWeight": 400,
          "lineHeight": 1.5,
