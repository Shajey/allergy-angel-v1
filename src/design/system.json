        "md": {
          "width": "1.5rem",
          "height": "1.5rem",
          "borderWidth": "2px"
        },
        "lg": {
          "width": "2.5rem",
          "height": "2.5rem",
          "borderWidth": "3px"
        }
      },
      "colors": {
        "primary": "#3B82F6",
        "secondary": "#64748B",
        "light": "#E2E8F0"
      }
    },

    "layout": {
      "sidebar": {
        "width": "240px",
        "collapsedWidth": "64px",
        "backgroundColor": "#1E293B",
        "textColor": "#FFFFFF",
        "padding": "1rem"
