      "mainContent": {
        "maxWidth": "1440px",
        "padding": "2rem",
        "backgroundColor": "#F8FAFC"
      },
      "gridSystem": {
        "columns": 12,
        "gutter": "1.5rem",
        "breakpoints": {
          "xs": "0px",
          "sm": "640px",
          "md": "768px",
          "lg": "1024px",
          "xl": "1280px",
          "2xl": "1536px"
        }
      }
    },

    "formLayout": {
      "field": {
        "marginBottom": "1.25rem",
        "display": "flex",
        "flexDirection": "column",
        "gap": "0.5rem"
      },
      "label": {
        "fontSize": "0.875rem",
        "fontWeight": 500,
        "color": "#1E293B",
        "marginBottom": "0.375rem",
        "display": "block"
      },
      "labelSpacing": {
        "gap": "0.375rem"
      },
      "requiredIndicator": {
        "color": "#EF4444",
        "marginLeft": "0.25rem"
      },
      "helperText": {
        "fontSize": "0.75rem",
        "color": "#64748B",
        "marginTop": "0.375rem",
        "lineHeight": 1.5
      },
      "errorText": {
        "fontSize": "0.75rem",
        "color": "#EF4444",
        "marginTop": "0.375rem",
        "lineHeight": 1.5,
        "display": "flex",
        "alignItems": "center",
        "gap": "0.25rem"
      },
      "fieldGap": "1.25rem",
      "sectionSpacing": {
        "marginTop": "2rem",
        "marginBottom": "1.5rem"
