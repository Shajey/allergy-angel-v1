      "title": {
        "fontSize": "1.125rem",
        "fontWeight": 600,
        "color": "#1E293B",
        "marginBottom": "0.5rem"
      },
      "description": {
        "fontSize": "0.875rem",
        "color": "#64748B",
        "maxWidth": "28rem",
        "lineHeight": 1.5
      },
      "action": {
